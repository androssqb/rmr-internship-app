<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/post_graph"
    app:startDestination="@id/post_fragment">

    <fragment
        android:id="@+id/post_fragment"
        android:name="com.redmadrobot.app.ui.post.PostFragment"
        android:label="PostFragment"
        tools:layout="@layout/fragment_post">
        <action
            android:id="@+id/action_post_fragment_to_google_map_fragment"
            app:destination="@id/google_map_fragment" />
    </fragment>
    <fragment
        android:id="@+id/google_map_fragment"
        android:name="com.redmadrobot.app.ui.googlemap.GoogleMapFragment"
        android:label="GoogleMapFragment"
        tools:layout="@layout/fragment_google_map">
        <argument
            android:name="mapMarker"
            app:argType="com.redmadrobot.app.model.MapMarker"
            app:nullable="true" />
    </fragment>
</navigation>
